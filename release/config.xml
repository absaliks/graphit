<?xml version="1.0" encoding="UTF-8"?>
<config>
  <field-delimiter>SEMICOLON_SPACE</field-delimiter>
  <time-format>HH:mm:ss</time-format>
  <line-delimiter>CRLF</line-delimiter>
  <date-format>dd.MM.YYYY</date-format>
  <table-visible>1</table-visible>
  <launch-visible>1</launch-visible>
  <datetime-format>dd.MM.YYYY HH:mm:ss</datetime-format>
  <recent>
    <item>E:\_dev\Java\Workspace\graphit\bin\00012699.log</item>
